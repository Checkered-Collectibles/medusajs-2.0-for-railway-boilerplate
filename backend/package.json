    "jest": "^29.7.0",
    "prop-types": "^15.8.1",
    "react-email": "^5.0.6",
    "react-router-dom": "6.20.1",
    "ts-node": "^10.9.2",
    "typescript": "^5.9.3",
    "vite": "^5.4.21"
  },
  "packageManager": "pnpm@9.10.0",
  "engines": {
    "node": "20.x",
    "pnpm": "9.10.0"
  },
