    "cors": "^2.8.5",
    "ioredis": "^5.8.2",
    "medusajs-launch-utils": "0.0.18",
    "minio": "^8.0.6",
    "razorpay": "^2.9.6",
    "react": "^18.3.1",
    "react-dom": "^18.3.1",
    "resend": "6.9.1",
    "ulid": "^2.4.0",
    "zod": "^3.25.76"
